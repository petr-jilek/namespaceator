{
  "files.eol": "\n",
  "workbench.tree.indent": 14,
  "explorer.compactFolders": false,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[csharp]": {
    "editor.defaultFormatter": "csharpier.csharpier-vscode"
  },
  "[xml]": {
    "editor.defaultFormatter": "csharpier.csharpier-vscode"
  },
  "files.exclude": {
    "**/bin": true,
    "**/obj": true
  },
  "files.associations": {
    "*.fsproj": "xml"
  }
}
